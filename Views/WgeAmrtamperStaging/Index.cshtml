@model IEnumerable<TestClusterProj.Models.WgeAmrtamperStaging>

<h2>WGE AMRTamperStaging Data</h2>

<table class="table">
    <thead>
        <tr>
            <th>Category</th>
            <th>DiscTime</th>
            <th>EventTime</th>
            <th>MeterName</th>
            <th>TamperType</th>
            <!-- Add other fields as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Category</td>
                <td>@item.DiscTime</td>
                <td>@item.EventTime</td>
                <td>@item.MeterName</td>
                <td>@item.TamperType</td>
                <!-- Add other fields as needed -->
            </tr>
        }
    </tbody>
</table>
